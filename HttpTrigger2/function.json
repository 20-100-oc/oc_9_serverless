{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }, 
    {
      "name": "recsFile",
      "type": "blob",
      "dataType": "binary",
      "path": "data-container/recs_idx_5.npy",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    }, 
    {
      "name": "embeddings",
      "type": "blob",
      "dataType": "binary",
      "path": "data-container/article_embeddings.pickle",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    }, 
    {
      "name": "timeClick",
      "type": "blob",
      "dataType": "binary",
      "path": "data-container/time_click.pkl",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    }
  ]
}




